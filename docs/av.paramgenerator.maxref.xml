<c74object name="av.paramgenerator">

	<digest>
		Generate parameter objects for slabs
	</digest>

	<description>
		Generate all the parameters that are listed in the shader file of a jit.gl.slab with Alt/Option+Shift+click on the object.
	</description>

	<!--METADATA-->
	<metadatalist>
		<metadata name="author">Timo Hoogland</metadata>
		<metadata name="tag">av-toolbox</metadata>
		<metadata name="tag">max</metadata>
		<metadata name="tag">audiovisual</metadata>
		<metadata name="tag">analysis</metadata>
		<metadata name="tag">msp</metadata>
		<metadata name="tag">jitter</metadata>
	</metadatalist>

	<!--INLETS-->
	<inletlist>
	</inletlist>

	<!--OUTLETS-->
	<outletlist>
	</outletlist>

	<!-- ARGUMENTS-->
	<objarglist>
		<!-- <objarg name="enable" optional="1" type="int">
			<digest>
				Enable/disable the processing on load (default = 1)
			</digest>
			<description>
				Enable/disable the processing on load (default = 1)
			</description>
		</objarg> -->
	</objarglist>

	<!--MESSAGES-->
	<methodlist>
		<method name="int">
			<arglist>
				<arg name="int" optional="1" type="int" />
			</arglist>
			<digest>
				Enable/disable the parameter generator (default = 1)
			</digest>
			<description>
				Enable/disable the parameter generator (default = 1)
			</description>
		</method>
	</methodlist>

	<!--ATTRIBUTES-->
	<attributelist>
		<attribute name="enable" get='0' set='1' type='bool' size='2'>
			<arglist>
				<arg name="enable" optional="1" type="bool" />
			</arglist>
			<digest>
				Enable/disable the parameter generator (default = 1)
			</digest>
			<description>
				Enable/disable the parameter generator (default = 1)
			</description>
		</attribute>
	</attributelist>

