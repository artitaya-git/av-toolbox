<c74object name="av.automation~">

	<digest>
		Draw automation for parameters controlled by a time interval in min:sec:ms. 
	</digest>

	<description>
		Draw automation for parameters controlled by a time interval in min:sec:ms. 
	</description>

	<!--METADATA-->
	<metadatalist>
		<metadata name="author">Timo Hoogland</metadata>
		<metadata name="tag">av-toolbox</metadata>
		<metadata name="tag">max</metadata>
		<metadata name="tag">audiovisual</metadata>
		<metadata name="tag">analysis</metadata>
		<metadata name="tag">msp</metadata>
		<metadata name="tag">jitter</metadata>
	</metadatalist>

	<!--INLETS-->
	<inletlist>
	</inletlist>

	<!--OUTLETS-->
	<outletlist>
	</outletlist>

	<!-- ARGUMENTS-->
	<objarglist>
		<objarg name="minutes" optional="1" type="int">
			<digest>
				Set phasor interval minutes (default = 0)
			</digest>
			<description>
				Set phasor interval minutes (default = 0)
			</description>
		</objarg>
		<objarg name="seconds" optional="1" type="int">
			<digest>
				Set phasor interval seconds (default = 1)
			</digest>
			<description>
				Set phasor interval seconds (default = 1)
			</description>
		</objarg>
		<objarg name="milliseconds" optional="1" type="float">
			<digest>
				Set phasor interval milliseconds (default = 0)
			</digest>
			<description>
				Set phasor interval milliseconds (default = 0)
			</description>
		</objarg>
		<objarg name="transport" optional="1" type="symbol">
			<digest>
				A named transport
			</digest>
			<description>
				Set a named transport to lock the automation to (default = global)
			</description>
		</objarg>
	</objarglist>

	<!--MESSAGES-->
	<methodlist>
			<method name="float">
			<arglist>
				<arg name="float" optional="1" type="float" />
			</arglist>
			<digest>
				Set phasor interval
			</digest>
			<description>
				Function depends on inlet. <br/>
				<br/>
				Inlet 1: Set phasor interval minutes (default = 0)<br/>
				Inlet 2: Set phasor interval seconds (default = 0) <br/>
				Inlet 3: Set phasor interval milliseconds (default = 0)<br/>
			</description>
		</method>
		<method name="int">
			<arglist>
				<arg name="int" optional="1" type="int" />
			</arglist>
			<digest>
				Set phasor interval
			</digest>
			<description>
				Function depends on inlet. <br/>
				<br/>
				Inlet 1: Set phasor interval minutes (default = 0)<br/>
				Inlet 2: Set phasor interval seconds (default = 0) <br/>
				Inlet 3: Set phasor interval milliseconds (default = 0)<br/>
			</description>
		</method>
		<method name="list">
			<arglist>
				<arg name="list" optional="1" type="list" />
			</arglist>
			<digest>
				Set phasor interval as a list of minutes seconds milliseconds
			</digest>
			<description>
				Set phasor interval as a list of minutes seconds milliseconds
			</description>
		</method>
	</methodlist>

